<h2>Search Genres</h2>

<form action="/genres" method="GET">
  <label for="">Name</label>
  <input type="text" name="name" value="<%=searchOptions.name%>" />
  <button type="submit">Search</button>
</form>
<br />
<%genres.forEach(genre=>{%>
<div><%=genre.name%></div>
<a href="/genres/<%= genre.id%>">View</a>
<a href="/genres/<%= genre.id%>/edit">Edit</a>
<%- include('../partials/deleteForm', {url:`/genres/${genre.id}`})%> <%}) %>
